{
	"name": "PvX Rate",
	"author": [
		"gcardinal",
		"Hhhippo",
		"Alexia E. Smith",
		"Cameron Chunn"
	],
	"descriptionmsg": "pvxrate_description",
	"license-name": "GPL-2.0-or-later",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"GroupPermissions": {
		"user": {
			"ratebuild": true
		}
	},
	"SpecialPages": {
		"UserRatings": "SpecialUserRatings",
		"RecentRatings": "SpecialRecentRatings"
	},
	"Actions": {
		"rate": "RateAction"
	},
	"MessagesDirs": {
		"PvXRate": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"RateAction": "classes/RateAction.php",
		"PvXRateHooks": "PvXRate.hooks.php",
		"SpecialUserRatings": "specials/SpecialUserRatings.php",
		"SpecialRecentRatings": "specials/SpecialRecentRatings.php"
	},
	"ResourceModules": {
		"ext.pvxrate": {
			"styles": [
				"css/pvxrate.css"
			],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "/",
		"remoteExtPath": "PvXRote"
	},
	"config": {
		"PvXRateBuildNamespace": "NS_BUILD",
		"PvXRateBuildTalkNamespace": "NS_BUILD_TALK",
		"PvXRateEditsRequired": 2
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": [
			"PvXRateHooks::onLoadExtensionSchemaUpdates"
		]
	},
	"manifest_version": 1
}
